<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="andyshiu.app.uitest.Page2" >

    <LinearLayout
        android:id="@+id/mmiv_down_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="#85000000"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="1dp"
            android:background="#FFFFFF" >

            <TextView
                android:id="@+id/page2_textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="登入頁面"
                android:textAppearance="?android:attr/textAppearanceMedium" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/page2_textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/TextView14" />

            <Spinner
                android:id="@+id/login_spinner1"
                android:layout_width="131dp"
                android:layout_height="58dp"
                android:layout_weight="2.20"
                android:ems="5"
                android:inputType="textPersonName" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/page2_textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/TextView6" />

            <EditText
                android:id="@+id/login_editText1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="3.03"
                android:ems="5"
                android:inputType="textPersonName" >

                <requestFocus />
            </EditText>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/page2_textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/TextView4" />

            <EditText
                android:id="@+id/login_editText2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="3.03"
                android:ems="5"
                android:inputType="textPersonName" >
            </EditText>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/p1231"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/TextView15" />

            <Spinner
                android:id="@+id/login_spinner2"
                android:layout_width="129dp"
                android:layout_height="wrap_content"
                android:layout_weight="2.20"
                android:ems="5"
                android:inputType="textPersonName" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF" >
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginTop="1dp"
            android:background="#FFFFFF" >

            <RelativeLayout
                android:id="@+id/login_button1"
                android:layout_width="90dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:background="#34CF95"
                android:clickable="true"
                android:focusable="true" >

                <TextView
                    android:id="@+id/page2_textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/Button1a"
                    android:textColor="#FFFFFF" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/login_button2"
                android:layout_width="90dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:background="#34CF95"
                android:clickable="true"
                android:focusable="true" >

                <TextView
                    android:id="@+id/page2_textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/Button4"
                    android:textColor="#FFFFFF" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF" >

            <RelativeLayout
                android:id="@+id/login_button3"
                android:layout_width="90dp"
                android:layout_height="30dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="#EF5F45"
                android:clickable="true"
                android:focusable="true" >

                <TextView
                    android:id="@+id/page2_textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/Button3"
                    android:textColor="#FFFFFF" />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/login_textView11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/mmiv_down_ly"
        android:layout_weight="0.05"
        android:gravity="center"
        android:textColor="@color/red"
        android:textSize="@dimen/text_large"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_weight="2"
        android:background="#FFFFFF" >
    </LinearLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/login_textView11"
        android:layout_alignBottom="@+id/login_textView11"
        android:layout_centerHorizontal="true"
        android:text="VER 1.5.3"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>